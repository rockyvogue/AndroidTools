<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android"
    >
    
	<item android:left="10dp"  android:right="10dp">
	  <shape android:shape="rectangle"  
		   android:useLevel="false">
		     <stroke android:width="10px" android:color="#FF004991"/>
		     <!-- <solid android:color="#FF004991" /> -->
		  </shape>
	</item>
	
	
	<item >
		  <shape android:shape="ring"  android:thicknessRatio="2.0"
		      android:innerRadius="0dp" 
		   android:useLevel="false">
		     <stroke android:width="3dp" android:color="#FFFF4991"/>
		     <solid android:color="#FFFF4991" />
		  </shape>
	</item> 
	
</layer-list> 