<?xml version="1.0" encoding="utf-8"?>
<com.spt.carengine.view.WiFiStaticIPView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/wifi_static_ip_ly_main"
    style="@style/Height_Width_match" >

    <include
        android:id="@+id/back_bar"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_alignParentBottom="true"
        app:layout_heightPercent="18.26%h"
        layout="@layout/return_bar" />

    <com.spt.carengine.view.PercentLinearLayout
        style="@style/Height_Width_match"
        android:layout_above="@id/back_bar"
        android:orientation="vertical" >

        <com.spt.carengine.view.PercentLinearLayout
            style="@style/Height_Width_0_match"
            android:layout_below="@id/back_bar"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/wifi_static_ip_address"
                style="@style/Height_Width_wrap"
                android:layout_centerVertical="true"
                app:layout_marginLeftPercent="7.09%w"
                app:layout_textSizePercent="40%h"
                android:text="@string/wifi_static_ip_address_tv"
                android:textColor="@color/text_color_selector" />

            <EditText
                android:id="@+id/wifi_static_edit_address"
                style="@style/Height_Width_0"
                app:layout_heightPercent="12.15%h"
                app:layout_marginLeftPercent="39.27%w"
                app:layout_marginTopPercent="2.02%h"
                app:layout_textSizePercent="7.29%h"
                app:layout_widthPercent="32.20%w"
                android:background="@color/return_bar_bg"
                android:text="@string/wifi_proxy_settings_edit_servers_tv" />
        </com.spt.carengine.view.PercentLinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            app:layout_marginLeftPercent="3.04%w"
            app:layout_marginRightPercent="3.04%w"
            android:background="#375A73" />

        <com.spt.carengine.view.PercentLinearLayout
            style="@style/Height_Width_0_match"
            android:layout_below="@id/back_bar"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/wifi_static_gateway"
                style="@style/Height_Width_wrap"
                android:layout_centerVertical="true"
                app:layout_marginLeftPercent="7.09%w"
                app:layout_textSizePercent="40%h"
                android:text="@string/wifi_static_gateway_tv"
                android:textColor="@color/text_color_selector" />

            <EditText
                android:id="@+id/wifi_static_edit_gateway"
                style="@style/Height_Width_0"
                app:layout_heightPercent="12.15%h"
                app:layout_marginLeftPercent="39.27%w"
                app:layout_marginTopPercent="2.02%h"
                app:layout_textSizePercent="7.29%h"
                app:layout_widthPercent="32.20%w"
                android:background="@color/return_bar_bg"
                android:text="@string/wifi_proxy_settings_edit_servers_tv" />
        </com.spt.carengine.view.PercentLinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            app:layout_marginLeftPercent="3.04%w"
            app:layout_marginRightPercent="3.04%w"
            android:background="#375A73" />

        <com.spt.carengine.view.PercentLinearLayout
            style="@style/Height_Width_0_match"
            android:layout_below="@id/back_bar"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/wifi_static_netmask"
                style="@style/Height_Width_wrap"
                android:layout_centerVertical="true"
                app:layout_marginLeftPercent="7.09%w"
                app:layout_textSizePercent="40%h"
                android:text="@string/wifi_static_netmask_tv"
                android:textColor="@color/text_color_selector" />

            <EditText
                android:id="@+id/wifi_static_edit_netmask"
                style="@style/Height_Width_0"
                app:layout_heightPercent="12.15%h"
                app:layout_marginLeftPercent="39.27%w"
                app:layout_marginTopPercent="2.02%h"
                app:layout_textSizePercent="7.29%h"
                app:layout_widthPercent="32.20%w"
                android:background="@color/return_bar_bg"
                android:text="@string/wifi_static_edit_netmask_tv" />
        </com.spt.carengine.view.PercentLinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            app:layout_marginLeftPercent="3.04%w"
            app:layout_marginRightPercent="3.04%w"
            android:background="#375A73" />

        <com.spt.carengine.view.PercentLinearLayout
            style="@style/Height_Width_0_match"
            android:layout_below="@id/back_bar"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/wifi_static_domain_name1"
                style="@style/Height_Width_wrap"
                android:layout_centerVertical="true"
                app:layout_marginLeftPercent="7.09%w"
                app:layout_textSizePercent="40%h"
                android:text="@string/wifi_static_domain_name1_tv"
                android:textColor="@color/text_color_selector" />

            <EditText
                android:id="@+id/wifi_static_edit_domain_name1"
                style="@style/Height_Width_0"
                app:layout_heightPercent="12.15%h"
                app:layout_marginLeftPercent="39.27%w"
                app:layout_marginTopPercent="2.02%h"
                app:layout_textSizePercent="7.29%h"
                app:layout_widthPercent="32.20%w"
                android:background="@color/return_bar_bg"
                android:text="@string/wifi_proxy_settings_edit_servers_tv" />
        </com.spt.carengine.view.PercentLinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            app:layout_marginLeftPercent="3.04%w"
            app:layout_marginRightPercent="3.04%w"
            android:background="#375A73" />

        <com.spt.carengine.view.PercentLinearLayout
            style="@style/Height_Width_0_match"
            android:layout_below="@id/back_bar"
            android:layout_weight="1"
            android:orientation="horizontal" >

            <TextView
                android:id="@+id/wifi_static_domain_name2"
                style="@style/Height_Width_wrap"
                android:layout_centerVertical="true"
                app:layout_marginLeftPercent="7.09%w"
                app:layout_textSizePercent="40%h"
                android:text="@string/wifi_static_domain_name2_tv"
                android:textColor="@color/text_color_selector" />

            <EditText
                android:id="@+id/wifi_static_edit_domain_name2"
                style="@style/Height_Width_0"
                app:layout_heightPercent="12.15%h"
                app:layout_marginLeftPercent="39.27%w"
                app:layout_marginTopPercent="2.02%h"
                app:layout_textSizePercent="7.29%h"
                app:layout_widthPercent="32.20%w"
                android:background="@color/return_bar_bg"
                android:text="@string/wifi_proxy_settings_edit_servers_tv" />
        </com.spt.carengine.view.PercentLinearLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            app:layout_marginLeftPercent="3.04%w"
            app:layout_marginRightPercent="3.04%w"
            android:background="#375A73" />
    </com.spt.carengine.view.PercentLinearLayout>

</com.spt.carengine.view.WiFiStaticIPView>