<?xml version="1.0" encoding="utf-8"?>
<com.spt.carengine.view.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#264863" >

    <include
        android:id="@+id/back_bar"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_alignParentBottom="true"
        app:layout_heightPercent="18.26%h"
        layout="@layout/return_bar" />

    <com.spt.carengine.view.PercentRelativeLayout
        android:id="@+id/pay"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/back_bar"
        android:layout_alignParentTop="true"
        app:layout_heightPercent="65.37%h"
        app:layout_marginLeftPercent="1.96%w"
        app:layout_marginRightPercent="1.96%w"
        app:layout_marginTopPercent="2.63%h" >

        <com.spt.carengine.view.PercentRelativeLayout
            android:id="@+id/pray0"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_heightPercent="17.32%h"
            android:background="#162C3C" >

            <TextView
                android:id="@+id/tv1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                app:layout_marginLeftPercent="2.72%w"
                app:layout_textSizePercent="45.71%h"
                android:background="#162C3C"
                android:gravity="start|center"
                android:text="@string/user_tv1"
                android:textColor="#FFFFFF" />
        </com.spt.carengine.view.PercentRelativeLayout>

        <View
            android:id="@+id/v1"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@id/pray0"
            app:layout_marginBottomPercent="0.08%h"
            app:layout_marginTopPercent="0.08%h"
            android:background="@drawable/bar" />

        <com.spt.carengine.view.PercentRelativeLayout
            android:id="@+id/pray1"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_below="@id/v1"
            app:layout_heightPercent="19.80%h" >

            <ImageView
                android:id="@+id/img_new_hand"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerInParent="true"
                app:layout_heightPercent="42.50%h"
                app:layout_marginRightPercent="2.72%w"
                app:layout_widthPercent="5.497%w"
                android:contentDescription="@null"
                android:scaleType="fitCenter"
                android:src="@drawable/clouddog_icon_selected" />

            <TextView
                android:id="@+id/tv_new_hand"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_toLeftOf="@id/img_new_hand"
                android:layout_toStartOf="@id/img_new_hand"
                app:layout_marginLeftPercent="2.72%w"
                app:layout_textSizePercent="40%h"
                android:gravity="start|center"
                android:text="@string/tv_new_hand"
                android:textColor="@color/album_text_selector" />
        </com.spt.carengine.view.PercentRelativeLayout>

        <View
            android:id="@+id/v2"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@id/pray1"
            app:layout_marginBottomPercent="0.08%h"
            app:layout_marginRightPercent="1.96%w"
            app:layout_marginTopPercent="0.08%h"
            android:background="@drawable/bar" />

        <com.spt.carengine.view.PercentRelativeLayout
            android:id="@+id/pray2"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_below="@id/v2"
            app:layout_heightPercent="19.80%h" >

            <ImageView
                android:id="@+id/img_old_hand"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerInParent="true"
                app:layout_heightPercent="42.50%h"
                app:layout_marginRightPercent="2.72%w"
                app:layout_widthPercent="5.497%w"
                android:contentDescription="@null"
                android:scaleType="fitCenter"
                android:src="@drawable/clouddog_icon_selected" />

            <TextView
                android:id="@+id/tv_old_hand"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_toLeftOf="@id/img_old_hand"
                android:layout_toStartOf="@id/img_old_hand"
                app:layout_marginLeftPercent="2.72%w"
                app:layout_textSizePercent="40%h"
                android:gravity="start|center"
                android:text="@string/tv_old_hand"
                android:textColor="@color/album_text_selector" />
        </com.spt.carengine.view.PercentRelativeLayout>

        <View
            android:id="@+id/v3"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@id/pray2"
            app:layout_marginBottomPercent="0.08%h"
            app:layout_marginRightPercent="1.96%w"
            app:layout_marginTopPercent="0.08%h"
            android:background="@drawable/bar" />

        <com.spt.carengine.view.PercentRelativeLayout
            android:id="@+id/pray3"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_below="@id/v3"
            app:layout_heightPercent="19.80%h" >

            <ImageView
                android:id="@+id/img_expert"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_alignParentEnd="true"
                android:layout_alignParentRight="true"
                android:layout_centerInParent="true"
                app:layout_heightPercent="42.50%h"
                app:layout_marginRightPercent="2.72%w"
                app:layout_widthPercent="5.497%w"
                android:contentDescription="@null"
                android:scaleType="fitCenter"
                android:src="@drawable/clouddog_icon_selected" />

            <TextView
                android:id="@+id/tv_expert"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_toLeftOf="@id/img_expert"
                android:layout_toStartOf="@id/img_expert"
                app:layout_marginLeftPercent="2.72%w"
                app:layout_textSizePercent="40%h"
                android:gravity="start|center"
                android:text="@string/tv_expert"
                android:textColor="@color/album_text_selector" />
        </com.spt.carengine.view.PercentRelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_below="@id/pray3"
            app:layout_marginBottomPercent="0.08%h"
            app:layout_marginRightPercent="1.96%w"
            app:layout_marginTopPercent="0.08%h"
            android:background="@drawable/bar" />
    </com.spt.carengine.view.PercentRelativeLayout>

</com.spt.carengine.view.PercentRelativeLayout>