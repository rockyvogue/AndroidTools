<?xml version="1.0" encoding="utf-8"?>
<com.spt.carengine.view.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="0dp"
    app:layout_heightPercent="16.19%h" >

    <TextView
        android:id="@+id/system_settings_List1_item_title"
        style="@style/Height_Width_wrap"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        app:layout_marginLeftPercent="7.09%w"
        app:layout_textSizePercent="40%h"
        android:text="@string/wifi_collision_detection"
        android:textColor="@color/text_color_selector" />

    <SeekBar
        android:id="@+id/system_settings_List1_item_seekbar"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="6dp"
        android:layout_marginRight="6dp"
         app:layout_marginRightPercent="1.96%w"
        app:layout_widthPercent="65.45%w"
        android:max="100"
        android:maxHeight="10dip"
        android:minHeight="10dip"
        android:paddingLeft="30dp"
        android:paddingRight="30dp"
        android:progress="0"
        android:progressDrawable="@drawable/wifi_settings_seekbar"
        android:thumb="@drawable/fm_channel_but_n"
        android:thumbOffset="28dp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_alignParentBottom="true"
        app:layout_marginLeftPercent="3.04%w"
        app:layout_marginRightPercent="3.04%w"
        android:background="#375A73" />

</com.spt.carengine.view.PercentRelativeLayout>