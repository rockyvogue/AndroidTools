<?xml version="1.0" encoding="utf-8"?>
<com.spt.carengine.view.PercentRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    style="@style/Height_Width_match"
    android:background="@drawable/record_file_bg" >

    <ImageView
        android:id="@+id/fm_left_imageView"
        style="@style/Height_Width_0_wrap"
        app:layout_heightPercent="12.96%h"
        app:layout_marginLeftPercent="15.58%w"
        app:layout_marginTopPercent="23.08%h"
        app:layout_widthPercent="11.38%w"
        android:scaleType="fitXY"
        android:src="@drawable/fm_previous_but_n" />

    <ImageView
        android:id="@+id/fm_right_imageView"
        style="@style/Height_Width_0"
        android:layout_alignParentRight="true"
        app:layout_heightPercent="12.96%h"
        app:layout_marginRightPercent="15.58%w"
        app:layout_marginTopPercent="23.08%h"
        app:layout_widthPercent="11.38%w"
        android:scaleType="fitXY"
        android:src="@drawable/fm_next_but_n" />

    <TextView
        android:id="@+id/fm_change_textView1"
        style="@style/Height_Width_wrap"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@id/fm_right_imageView"
        android:layout_toLeftOf="@id/fm_right_imageView"
        app:layout_marginRightPercent="5.82%w"
        app:layout_marginTopPercent="16.19%h"
        app:layout_textSizePercent="8.10%h"
        android:gravity="center"
        android:text="@string/mhz_unit"
        android:textColor="#13D0A1" />

    <TextView
        android:id="@+id/fm_change_textView"
        style="@style/Height_Width_wrap"
        android:layout_alignBaseline="@id/fm_change_textView1"
        android:layout_alignBottom="@id/fm_change_textView1"
        android:layout_toLeftOf="@id/fm_change_textView1"
        app:layout_marginTopPercent="16.59%h"
        app:layout_textSizePercent="12.96%h"
        android:paddingTop="12dp"
        android:text="@string/fm_88_0"
        android:textColor="#13D0A1" />

    <SeekBar
        android:id="@+id/fm_seekbar_def"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        app:layout_marginBottomPercent="34.01%h"
        app:layout_marginLeftPercent="12.57%w"
        app:layout_marginRightPercent="12.57%w"
        android:background="@drawable/fm_channel_bg"
        android:max="2050"
        android:paddingBottom="6dp"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingTop="6dp"
        android:progress="0"
        android:progressDrawable="@drawable/fm_seekbar_define_color_style"
        android:thumb="@drawable/fm_channel_but_n" />

    <TextView
        android:id="@+id/textView1"
        style="@style/Height_Width_wrap"
        android:layout_above="@id/fm_seekbar_def"
        android:layout_alignLeft="@+id/fm_left_imageView"
        app:layout_textSizePercent="4.45%h"
        android:text="@string/fm_87_50"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/textView2"
        style="@style/Height_Width_wrap"
        android:layout_alignBaseline="@id/textView1"
        android:layout_alignBottom="@id/textView1"
        android:layout_toRightOf="@id/textView1"
        app:layout_textSizePercent="2.83%h"
        android:text="@string/mhz_unit"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/textView3"
        style="@style/Height_Width_wrap"
        android:layout_above="@id/fm_seekbar_def"
        android:layout_alignBaseline="@id/textView1"
        android:layout_alignBottom="@id/textView1"
        android:layout_alignRight="@id/fm_right_imageView"
        android:layout_toLeftOf="@id/fm_right_imageView"
        app:layout_textSizePercent="2.83%h"
        android:text="@string/mhz_unit"
        android:textColor="#ffffff" />

    <TextView
        android:id="@+id/textView4"
        style="@style/Height_Width_wrap"
        android:layout_above="@id/fm_seekbar_def"
        android:layout_alignBaseline="@id/textView1"
        android:layout_alignBottom="@id/textView3"
        android:layout_toLeftOf="@+id/textView3"
        app:layout_textSizePercent="4.45%h"
        android:text="@string/fm_108_00"
        android:textColor="#ffffff" />

    <ImageView
        android:id="@+id/fm_imageView1"
        style="@style/Height_Width_0_wrap"
        android:layout_alignBottom="@id/textView1"
        android:layout_marginBottom="3dp"
        android:layout_toLeftOf="@id/textView4"
        android:layout_toRightOf="@id/textView2"
        app:layout_heightPercent="4.45%h"
        app:layout_widthPercent="46.07%w"
        android:src="@drawable/fm_staff_bg" />

    <include
        android:id="@+id/back_bar"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_alignParentBottom="true"
        app:layout_heightPercent="18.26%h"
        layout="@layout/return_bar" />

</com.spt.carengine.view.PercentRelativeLayout>