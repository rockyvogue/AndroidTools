<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 透明的ImageButton -->
    <style name="Widget.ImageButton.Translate" parent="@android:style/Widget.ImageButton">
        <item name="android:background">@null</item>
    </style>

    <style name="Widget.CompoundButton.RadioButton.Ring" parent="@android:style/Widget.CompoundButton.RadioButton">

        <!-- radio_ring_selector  radio_ring_selected radio_ring_selector_test -->
        <item name="android:background">@drawable/radio_ring_selector_test</item>
        <item name="android:button">@null</item>
        <item name="android:width">153dp</item>
        <item name="android:height">153dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <!-- dial pad Style -->
    <style name="DialtactsDialpadButtonStyle">
        <item name="android:layout_width">0dip</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_weight">1</item>
        <item name="android:background">?android:attr/selectableItemBackground</item>
        <item name="android:soundEffectsEnabled">false</item>
    </style>

    <style name="AppBaseTheme" parent="@android:style/Theme.Light"></style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="PopupAnimation" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/popwindow_enter</item>
        <item name="android:windowExitAnimation">@anim/popwindow_exit</item>
    </style>

    <style name="navigation_bottom_radio">

        <!-- 内部组件的排列 -->

        <item name="android:gravity">bottom</item>

        <!-- 宽度 -->

        <item name="android:layout_width">match_parent</item>

        <!-- 高度 -->

        <item name="android:layout_height">match_parent</item>

        <!-- 设置RadioButton的原来图片为空 -->

        <item name="android:button">@null</item>

        <!-- 与其他组件宽度占相同比重 -->
        <item name="android:layout_weight">1.0</item>
    </style>

    <style name="checkbox_fmsend" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:button">@drawable/checkbox_fm_switch</item>
    </style>

    <!-- voice Theme -->
    <style name="G1_Font">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#ffeeeeee</item>
    </style>

    <style name="Voice_Msg_Text_Theme">
        <item name="android:textSize">@dimen/voice_text_size</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:shadowColor">@color/semi_transparent_w</item>
        <item name="android:shadowDy">-1.0</item>
        <item name="android:shadowRadius">0.2</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:focusable">true</item>
    </style>

    <style name="DirectoryLoading">
        <item name="android:indeterminate">true</item>
        <item name="android:indeterminateDrawable">@drawable/loading_drawable</item>
        <item name="android:indeterminateDuration">1000</item>
        <item name="android:indeterminateOnly">true</item>
    </style>

    <!-- 拨号界面的按钮样式 -->
    <style name="Call_Button_number">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@drawable/select_btn_bt_key</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>

    <!-- 拨号界面中联系人中的listview item 样式 -->
    <style name="Call_contact_item">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dip</item>
        <item name="android:layout_weight">2.5</item>
        <item name="android:gravity">left|center</item>
        <item name="android:textColor">#FFFFFF</item>
    </style>

    <!-- voice styles -->
    <declare-styleable name="FuctionView">
        <attr name="function_textColor" format="color" />
        <attr name="function_textSize" format="dimension" />
        <attr name="textWordMargin" format="dimension" />
        <attr name="textLineMargin" format="dimension" />
        <attr name="textBackground" format="integer" />
        <attr name="textPaddingLeft" format="dimension" />
        <attr name="textPaddingRight" format="dimension" />
        <attr name="textPaddingTop" format="dimension" />
        <attr name="textPaddingBottom" format="dimension" />
    </declare-styleable>

    <style name="ProgressBarHorizontalStyle" parent="android:style/Widget.ProgressBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:indeterminateDrawable">@android:drawable/progress_indeterminate_horizontal</item>
        <item name="android:progressDrawable">@drawable/progress_bar_drawable</item>
    </style>

    <style name="TextEditorDialog" parent="android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@null</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="PoiRatingBar" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/rating_bar_poi_full_filled</item>
    </style>

    <style name="Height_Width_match">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="Height_Width_wrap">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <style name="Height_Width_0">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
    </style>

    <style name="Height_Width_0_centerVertical">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">0dp</item>
    </style>

    <style name="Height_Width_match_0">
        <item name="android:layout_height">match_parent</item>
        <item name="android:layout_width">0dp</item>
    </style>

    <style name="Height_Width_0_wrap">
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_width">wrap_content</item>
    </style>

    <style name="Height_Width_0_match">
        <item name="android:layout_height">0dp</item>
        <item name="android:layout_width">match_parent</item>
    </style>
    
   <style name="Dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:background">#00000000</item>
    </style>
    
